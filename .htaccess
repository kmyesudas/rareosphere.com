RewriteEngine on
RewriteCond %{THE_REQUEST} /([^.]+)\.html [NC]
RewriteRule ^ /%1 [NC,L,R]

RewriteEngine On
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} ^www\. [NC]
RewriteRule ^ https://rareosphere.com%{REQUEST_URI} [L,R=301]

RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^ %{REQUEST_URI}.html [NC,L]



Redirect 301 /categories/Home-Office-Decor/3d-removable-wall-stickers /categories/home-office-decor/3d-removable-wall-stickers

RewriteEngine On

# Convert URL to lowercase for URLs within /Ladies-Products/ folder
RewriteCond %{REQUEST_URI} ^/categories/Ladies-Products/ [NC]
RewriteRule ^categories/Ladies-Products/(.*)$ /categories/ladies-products/$1 [R=301,L,NC]

# Redirect from uppercase to lowercase for the entire /Ladies-Products/ folder
RewriteRule ^(.*)$ /$1 [L,NE]

# Rewrite to lowercase
RewriteMap  lc int:tolower
RewriteRule  [A-Z] ${lc:$0} [R=301,L]
