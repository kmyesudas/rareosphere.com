RewriteEngine on
RewriteCond %{THE_REQUEST} /([^.]+)\.html [NC]
RewriteRule ^ /%1 [NC,L,R]

RewriteEngine On
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} ^www\. [NC]
RewriteRule ^ https://rareosphere.com%{REQUEST_URI} [L,R=301]

RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^ %{REQUEST_URI}.html [NC,L]



Redirect 301 /categories/Home-Office-Decor/3d-removable-wall-stickers /categories/home-office-decor/3d-removable-wall-stickers

Redirect 301 /categories/Bike-and-Car-Gadgets/(.*) /categories/bike-and-car-gadgets/lowercase($1)
Redirect 301 /categories/Daily-Life-Hack/(.*) /categories/daily-life-hack/lowercase($1)
Redirect 301 /categories/Health-and-Fitness/(.*) /categories/health-and-fitness/lowercase($1)
