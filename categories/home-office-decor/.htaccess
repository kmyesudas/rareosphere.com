# Ensure mod_rewrite is enabled (consult your server documentation if needed)
<IfModule mod_rewrite.c>

  # Rewrite Engine On (usually already enabled by default)
  RewriteEngine On

  # Check if requested URL starts with "/Home-Office-Decor/" (case-insensitive)
  RewriteCond %{REQUEST_URI} ^/Home-Office-Decor/(.*)$ [NC]

  # Redirect to lowercase version with appropriate function (replace with your server's actual function)
  RewriteRule ^(.+)$ /home-office-decor/lowercase($1) [L,R=301]

</IfModule>

# Add any other necessary .htaccess rules below this line (if applicable)
